{
	"name": "bjort theme",
	"type": "dark",
	"colors": {
		"foreground": "#afafaf",
		"editor.background": "#192733",
		"editor.foreground": "#d4d4d4",
		"editor.lineHighlightBackground" : "#213444",
		"editorLineNumber.foreground": "#546E7A",
		"editorGroupHeader.tabsBackground": "#192733",
		"editorGroupHeader.tabsBorder": "#213444",
		"editorWidget.background": "#111b23",
		"editorIndentGuide.background": "#546E7A",
		"editor.findMatchBackground": "#007accde",
		"editor.findMatchHighlightBackground": "#007acc6e",

		"activityBar.background": "#192733",
		"activityBarBadge.background": "#007acc",

		"sideBarTitle.foreground": "#afafaf",
		"sideBar.background": "#111b23",
		"sideBar.foreground": "#afafaf",
		"sideBar.border": "#192733",

		"panel.border": "#CCC02B",
		"panel.background": "#111b23",
		"panelTitle.activeForeground" : "#afafaf",
		"panelTitle.inactiveForeground" : "#afafaf",

		"tab.activeBackground": "#111b23",
		"tab.activeForeground": "#afafaf",
		"tab.activeBorder": "#CCC02B",
		"tab.inactiveBackground" : "#192733",
		"tab.inactiveForeground" : "#afafaf",

		"input.background": "#213444",
		"inputOption.activeBorder": "#85c58b",

		"dropdown.background": "#213444",
		"dropdown.foreground": "#afafaf",
		"dropdown.listBackground": "#213444",

		"titleBar.activeBackground": "#111b23",

		"statusBar.background": "#CCC02B",
		"statusBar.foreground": "#111b23",

		"list.activeSelectionBackground": "#192733",
		"list.activeSelectionForeground": "#afafaf",
		"list.hoverBackground": "#192733",
		"list.inactiveSelectionBackground": "#192733",
		"list.errorForeground": "#FF5370",
		"list.warningForeground": "#ff9d00"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"entity.other.inherited-class.js"
			],
			"settings": {
				"foreground": "#d6e5e5"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#CCC02B"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"meta.tag",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#779cad"
			}
		},
		{
			"name": "Return - JS",
			"scope": [
				"keyword.control.flow.js"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.separator.inheritance.php",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#d6e5e5"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"meta.tag.any.html",
				"meta.tag.other.html",
				"meta.tag.block.any.html",
				"markup.deleted.git_gutter",
				"punctuation.definition.tag",
